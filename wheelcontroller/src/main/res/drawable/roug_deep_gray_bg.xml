<?xml version="1.0" encoding="utf-8"?>
<inset
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:insetTop="0dip"
	android:insetBottom="0dip"
	android:insetLeft="0dip"
	android:insetRight="0dip">

	<shape>
		<gradient
			android:startColor="@color/color_White"
			android:centerColor="@color/color_White"
			android:endColor="@color/color_White"
			android:angle="270" />
		<stroke
			android:width="2dip"
			android:color="@color/color_White" />
		<corners
			android:radius="5dp" />
		<padding
			android:top="0dip"
			android:bottom="0dip"
			android:left="0dip"
			android:right="0dip" />
	</shape>
</inset>