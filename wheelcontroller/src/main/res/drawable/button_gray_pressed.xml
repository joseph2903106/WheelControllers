<?xml version="1.0" encoding="utf-8"?>
<inset
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:insetTop="1dip"
	android:insetBottom="1dip"
	android:insetLeft="1dip"
	android:insetRight="1dip">
	<shape>
		<gradient
			android:startColor="#7b7b7b"
			android:centerColor="#1a1a1a"
			android:endColor="#313131"
			android:angle="270" />
		<stroke
			android:width="1dip"
			android:color="#333333" />
		<corners
			android:radius="5dp" />
	</shape>
</inset>

